datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider  = "prisma-client-js"
}

model Post {
  id         Int      @id @default(autoincrement())
  user_id    Int
  timestamp  DateTime @default(now())
  visibility String
  content    String

  // Relationships
  // ...
}
